{
  "name": "zenon-desktop-app",
  "version": "1.0.0",
  "description": "ZENON veritabanı için desktop arama uygulaması",
  "main": "main.js",
  "scripts": {
    "start": "electron . --disable-gpu-cache --disable-disk-cache",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "cross-env NODE_ENV=development concurrently \"npm run dev\" \"wait-on http://localhost:3000 && npm run start\"",
    "electron:build": "npm run build && npm run start"
  },
  "keywords": [
    "electron",
    "react",
    "mssql",
    "zenon"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@amcharts/amcharts5": "^5.13.5",
    "electron": "^37.2.6",
    "mssql": "^10.0.2",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.0.0",
    "cross-env": "^10.0.0",
    "electron-vite": "^4.0.0",
    "vite": "^7.1.2",
    "wait-on": "^8.0.4"
  }
}
